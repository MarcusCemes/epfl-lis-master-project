<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="sendStatus">
    <Locals>
      <Local name="buffer" type="num" xsi:type="array" size="76" />
      <Local name="currentPosition" type="point" xsi:type="array" size="1" />
      <Local name="currentPose" type="joint" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  buffer[0] = MAGIC_HEADER
  buffer[1] = 1
  buffer[2] = sel(isSettled(), 1, 0)
  buffer[3] = sel(isEmpty(), 1, 0)

  currentPosition = here(endEffector, world)
  call encodePoint(currentPosition, buffer[4])

  currentPose = herej()
  call encodeJoint(currentPose, buffer[28])

  currentPose = getPositionErr()
  call encodeJoint(currentPose, buffer[52])

  sioSet(socket, buffer)
end
]]></Code>
  </Program>
</Programs>